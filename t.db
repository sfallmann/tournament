CREATE TABLE players (
    player_id integer NOT NULL,
    name text
);


ALTER TABLE public.players OWNER TO vagrant;

--
-- Name: participated; Type: VIEW; Schema: public; Owner: vagrant
--

CREATE VIEW participated AS
 SELECT matches.match_id AS match,
    players.player_id AS participant
   FROM matches,
    players
  WHERE ((players.player_id = matches.winner_id) OR (players.player_id = matches.loser_id))
  GROUP BY matches.match_id, players.player_id;


ALTER TABLE public.participated OWNER TO vagrant;

--
-- Name: player_summary; Type: VIEW; Schema: public; Owner: vagrant
--

CREATE VIEW player_summary AS
 SELECT participated.participant AS player,
    count(participated.match) AS total_matches,
    count(matches.winner_id) AS wins
   FROM (participated
     LEFT JOIN matches ON ((participated.participant = matches.winner_id)))
  GROUP BY participated.participant;


ALTER TABLE public.player_summary OWNER TO vagrant;

